# Kitty specific config
alias icat="kitty +kitten icat"

# Lunarvide 
alias lunarvide="NVIM_APPNAME=neovim/LunarVim neovide"

# Lvim configuration
alias lvimrc="lvim $HOME/.config/lvim/config.lua"

# Update and Upgrade Packages
alias autoupdate="sudo apt-get update && sudo apt-get upgrade -y"

# DFS Config
alias DFSrc="lvim $HOME/dotfiles/"

# GCP Configs
alias gcp-arete="gcloud config configurations activate arete-health"
alias gcp-jesse="gcloud config configurations activate home-reading-helper"

# Source zshrc
alias szsh="source ~/.zshrc"

